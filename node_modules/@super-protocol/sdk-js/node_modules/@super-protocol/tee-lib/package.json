{
  "name": "@super-protocol/tee-lib",
  "version": "0.1.3",
  "description": "The TEE TLB library",
  "tags": [
    "tee"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Super-Protocol/tee-trusted-loader.git"
  },
  "bugs": {
    "url": "https://github.com/Super-Protocol/tee-trusted-loader/issues"
  },
  "author": "Super Protocol",
  "license": "ISC",
  "engines": {
    "node": ">=14.0",
    "npm": ">=6.0"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "lib": "dist"
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "tsc -b",
    "build:clean": "rm -rf ./dist && tsc -b",
    "lint": "eslint --ext .ts src",
    "lint:fix": "eslint --ext .ts src --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk node_modules/.bin/jest -i"
  },
  "dependencies": {
    "@msgpack/msgpack": "^2.7.1",
    "buffer": "^6.0.3",
    "gzip-js": "^0.3.2"
  },
  "devDependencies": {
    "@types/gzip-js": "^0.3.3",
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.5",
    "ts-jest": "^27.1.2",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
